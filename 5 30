def 크기가1인_사각형을_가로로출력(n):
    for i in range(n):
        #사각형 출력하기
        print('@',end='')

    print()

def 넓이에맞는사각형출력(a,b):
    for i in range(a):
        크기가1인_사각형_가로로출력(b)
    print(); print();print()


r=int(input('넓이를 입력해주세요'))

for i in range(1,r+1):
    if r%1==0: and i*i <= r: #  약수 찾기
        넓아에맞는사각형출력(i,r//i)
        
