a = 1
while a<i**2+1:
    print (f' {a}')
    a+=1
    if i%3==0:
        c= a[0:3],
        c= a[3:6],
        c= a[6:9]
        print(c)
    elif i%4==0:
        c= a[0:4],
        c= a[4:8],
